load("@io_bazel_rules_go//go:def.bzl", "go_binary", "go_test")

licenses(["notice"])

go_binary(
    name = "main",
    srcs = ["main.go"],
)

go_test(
    name = "main_test",
    size = "small",
    srcs = ["main_test.go"],
    embed = [":main"],
    rundir = ".",
    deps = ["@com_github_google_go-cmp//cmp:go_default_library"],
)

exports_files(["main.go"])